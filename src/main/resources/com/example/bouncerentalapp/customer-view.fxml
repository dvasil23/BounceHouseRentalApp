<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.bouncerentalapp.CustomerController">
    <children>
        <TableView fx:id="customers" layoutY="83.0" prefHeight="200.0" prefWidth="602.0">
            <columns>
                <TableColumn fx:id="orderIDCol" prefWidth="52.5" text="order id" />
                <TableColumn fx:id="streetCol" prefWidth="37.5" text="street" />
                <TableColumn fx:id="cityCol" prefWidth="85.0" text="city" />
                <TableColumn fx:id="stateCol" prefWidth="45.5" text="state" />
                <TableColumn fx:id="zipCodeCol" prefWidth="58.0" text="zip code" />
                <TableColumn fx:id="firstNameCol" prefWidth="86.5" text="first name" />
                <TableColumn fx:id="lastNameCol" prefWidth="71.5" text="last name" />
                <TableColumn fx:id="phoneNumberCol" prefWidth="77.5" text="phone number" />
                <TableColumn fx:id="startDateCol" prefWidth="87.0" text="start date" />
            </columns>
        </TableView>
        <Label layoutX="241.0" layoutY="14.0" text="Customer info">
            <font>
                <Font name="System Bold" size="18.0" />
            </font>
        </Label>
        <Button layoutX="73.0" layoutY="334.0" mnemonicParsing="false" onAction="#signOut" text="sign out" />
        <Button layoutX="159.0" layoutY="334.0" mnemonicParsing="false" onAction="#seeOrderAudit" text="see order audit" />
    </children>
</AnchorPane>
